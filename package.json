{
  "name": "zintui",
  "private": false,
  "version": "0.0.7",
  "description": "zint UI library",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "files": [
