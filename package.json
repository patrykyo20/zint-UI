    "@babel/preset-react": "^7.12.10",
    "@eslint/js": "^9.9.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@types/node": "^22.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^9.9.0",
